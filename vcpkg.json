{
    "dependencies": [
        "zlib",
        {
            "name": "curl",
            "default-features": false,
            "features": ["ssl", "openssl", "http2"]
        },
        {
            "name": "libxml2",
            "default-features": false,
            "features": ["iconv"]
        }
    ],
    "vcpkg-configuration": {
        "overlay-ports": [
            "./extension-ci-tools/vcpkg_ports"
        ],
        "overlay-triplets": [
            "./extension-ci-tools/toolchains"
        ]
    }
}
